<h1><ins>Create a new report</ins> </h1>

<%= form_for @report do |f| %> 
    <%= f.label :title %><br>
    <%= f.text_field :title %> 
<br>
<br>
    <%= f.label :body %><br>
    <%= f.text_field :body %>
<br>
<br>
    <%= f.fields_for :markers do |marker| %>
        <%= marker.label :waterline_distance %><br>
        <%= marker.text_field :waterline_distance %><br/>
    <% end %>
<br>
    <%= f.submit %>

<% end %>

