<div>
    <%= form_for @follow do |f| %>
        <%= f.label :user_id %>
        <%= f.collection_select :user_id, @users, :id, :username %> 
        <br>
        <%= f.label :location_id %>
        <%= f.collection_select :location_id, @locations, :id, :zipcode %>
        <br>
        <%= f.submit "Submit" %>
    <% end %>
</div>